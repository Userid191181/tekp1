package OOPS;

public class ConstructorChaining {
	String s="Hello";
	int i=100;
	int age=50;
	ConstructorChaining() 
	{		
		System.out.println("Start of studens protal"); 
		System.out.println("---------");
	}
	
	ConstructorChaining(int i) 
	{
		this(); //will call previous constructor
		//this.i=i;
		System.out.println(i); 
		System.out.println("---------");
	}
	ConstructorChaining(int i, String s)
	{
		this(i); //will call previous constructor
		this.s=s;
		System.out.println(i);
		System.out.println(s);
		System.out.println("---------");
	}
	ConstructorChaining(int i, String s, int age)
	{
		this(i,s); //will call previous constructor
		this.age=age;
		System.out.println(age);
		System.out.println(i);
		System.out.println(s);
	}
	
	
public static void main(String[] args) {
	ConstructorChaining c=new ConstructorChaining(25678, "Hari", 25);  //only once object creation is done here.
}
}
